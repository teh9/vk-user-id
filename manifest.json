{
  "manifest_version": 3,
  "name": "VKcom user id getter",
  "description": "Allow to get user id in social network VK.com",
  "version": "1.0.0",
  "icons": {
    "128": "icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [ "https://vk.com/*" ],
      "js": [
        "jquery.min.js",
        "app.js"
      ],
      "css": [
        "assets/css/style.css"
      ]
    }
  ]
}
